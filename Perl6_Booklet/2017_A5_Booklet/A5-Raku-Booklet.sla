            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="128.347086614174" YPOS="3300.62204724409" OwnPage="5" ItemID="878919796" PTYPE="4" WIDTH="362.834645669291" HEIGHT="21.5999999999999" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="1" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L362.835 0 L362.835 21.6 L0 21.6 L0 0 Z" copath="M0 0 L362.835 0 L362.835 21.6 L0 21.6 L0 0 Z" gXpos="128.347086614172" gYpos="3300.6220472441" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
